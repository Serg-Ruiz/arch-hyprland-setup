{   
    "layer": "top",
    "modules-left": ["custom/launcher", "cpu", "memory", "tray"],
    "modules-right": ["clock"],
	
    "tray":{
	"icon-size":18,
	"spacing": 10
    },
    "cpu": {
        "interval": 15,
        "format": " {}%",
        "max-length": 10
    },
    "memory": {
        "interval": 30,
        "format": " {}%",
        "max-length": 10
    },
    "custom/launcher":{
        "format": " ",
        "on-click": "rofi -show drun",
        "on-click-right": "killall rofi"
    },

    "clock": {
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format": "{:%B %d, %A, %I:%M %p}"
    }
 }
